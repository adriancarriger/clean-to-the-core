{
  "name": "clean-to-the-core",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "deploy": "npm run docs && ng gh-pages:deploy --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\"",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' --excludeExternals ./src/app",
    "e2e": "protractor",
    "e2eTests": "export PROTRACTOR_ENV='local'; ng e2e",
    "fixCoverage": "node ./scripts/fix_coverage.js",
    "lint": "tslint \"src/**/*.ts\"",
    "pree2e": "webdriver-manager update; tsc --project e2e",
    "minCoverage": "node ./scripts/minimum_coverage.js",
    "start": "ng serve",
    "test": "ng lint && ng test --watch=false --code-coverage && npm run fixCoverage",
    "typedoc": "typedoc"
  },
  "config": {
    "pre-git": {
      "commit-msg": "conventional",
      "pre-push": [
        "ng lint"
      ]
    }
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.1.2",
    "@angular/compiler": "2.1.2",
    "@angular/core": "2.1.2",
    "@angular/forms": "2.1.2",
    "@angular/http": "2.1.2",
    "@angular/platform-browser": "2.1.2",
    "@angular/platform-browser-dynamic": "2.1.2",
    "@angular/router": "3.1.2",
    "angularfire2": "^2.0.0-beta.5",
    "core-js": "^2.4.1",
    "ng2-disqus": "git://github.com/adriancarriger/ng2-disqus.git",
    "rxjs": "5.0.0-rc.2",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/jasmine": "^2.2.30",
    "@types/node": "^6.0.42",
    "angular-cli": "1.0.0-beta.19-3",
    "codelyzer": "1.0.0-beta.3",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.7.0",
    "karma": "1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "lcov-parse": "0.0.10",
    "pre-git": "^3.10.0",
    "protractor": "4.0.10",
    "ts-node": "1.7.0",
    "tslint": "3.15.1",
    "typedoc": "^0.5.1",
    "typedoc-plugin-external-module-name": "^1.0.4",
    "typescript": "2.0.7",
    "webdriver-manager": "10.2.6"
  }
}
